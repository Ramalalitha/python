l=list(map(int,input().split()))
d={}
for i in l:
    if i not in d:
        d[i]=1
    else:
        d[i]=d[i]+1
m=list(d.values())
r=(max(m))
s=(min(m))
for i in d:
    if d[i]==r:
      print('max:',i)
      a=i
for j in d:
    if d[j]==s:
      print('min:',j)
      b=j
if a>b:
    print(a-b)
else:
    print(b-a)
